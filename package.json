    "jest": "^29.7.0",
    "mongodb": "^6.3.0",
    "mongodb-memory-server": "^10.1.4",
    "ts-jest": "^29.3.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "keywords": [
    "mongodb",
    "zod",
    "typescript",
    "validation",
    "schema",
    "orm"
  ],
  "author": "alesso-x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alesso-x/zod-mongo.git"
